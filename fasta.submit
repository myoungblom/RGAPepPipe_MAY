universe = vanilla
executable = VCFtoFasta.py
arguments = "$(RUN)_filter.vcf"

output = VCFtoFasta.out
error = VCFtoFasta.err
log = VCFtoFasta.log

should_transfer_files = YES
when_to_transfer_output = ON_EXIT
transfer_input_files = $(RUN)_filter.vcf

request_cpus = 1
request_memory = 1GB
request_disk = 1GB

queue
